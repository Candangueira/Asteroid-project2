<%- include ('./partials/header') %>

<section id="show-page">

    <h3>Name: <%= asteroid.name %>
    </h3>
    <p>Magnitude: <%= asteroid.absolute_magnitude_h %>
    </p>
    <p>Diameter: <%= asteroid.estimated_diameter.meters.estimated_diameter_min %>
    </p>
    <p>Is Hazardous?: <%= asteroid.is_potentially_hazardous_asteroid %>
    </p>
    
    <p>More details: <a href='<%= asteroid.nasa_jpl_url %>'><%= asteroid.nasa_jpl_url%></a></p>

    <p>List of days it will be closer to the Earth: <% asteroid.close_approach_data.map((dates)=> { %>
    <br>
     <%= dates.close_approach_date %>
    
     <%   }); %> </p>

<%- include ('./partials/footer') %>